{
  "name": "rollup-plugin-resolve-aliases",
  "version": "0.0.1",
  "description": "Resolves aliases with Rollup just like Webpack does",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "scripts": {
    "dev": "npm run std && rollup",
    "watch": "npm run std && rollup -w",
    "build": "npm run std && rollup",
    "std": "standard --verbose | snazzy"
  },
  "standard": {
    "ignore": [
      "lib",
      "rollup.config.js",
      "node_modules"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frostney/rollup-plugin-alias.git"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "resolve",
    "aliases"
  ],
  "author": "Andrew Kirichok",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frostney/rollup-plugin-alias/issues"
  },
  "homepage": "https://github.com/frostney/rollup-plugin-aliar#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "rollup": "^0.58.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-globals": "^1.2.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1"
  },
  "dependencies": {}
}
